<%= form_for(@kitten) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.number_field(:age, in: 1..50) %>

  <%= f.label :cuteness %>
  <%= f.number_field(:cuteness, in: 1..10) %>

  <%= f.label :softness %>
  <%= f.number_field(:softness, in: 1..10) %>

  <%= f.submit 'Submit' %>
<% end %>